<h1>Periodo</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Periodo_inicio</th>
            <td>{{ entity.periodoinicio|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Periodo_final</th>
            <td>{{ entity.periodofinal|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Porcentaje</th>
            <td>{{ entity.porcentaje }}</td>
        </tr>
        <tr>
            <th>Actividades_min</th>
            <td>{{ entity.actividadesmin }}</td>
        </tr>
        <tr>
            <th>Actividades_max</th>
            <td>{{ entity.actividadesmax }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admincloud_periodo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admincloud_periodo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admincloud_periodo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
