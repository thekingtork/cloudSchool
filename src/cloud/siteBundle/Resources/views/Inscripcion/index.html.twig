<h1>Inscripcion list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Inscripcion_inicio</th>
            <th>Inscripcion_final</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admincloud_inscripcion_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.inscripcioninicio %}{{ entity.inscripcioninicio|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.inscripcionfinal %}{{ entity.inscripcionfinal|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admincloud_inscripcion_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admincloud_inscripcion_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admincloud_inscripcion_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
