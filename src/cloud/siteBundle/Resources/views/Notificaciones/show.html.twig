<h1>Notificaciones</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Asunto</th>
            <td>{{ entity.asunto }}</td>
        </tr>
        <tr>
            <th>Mensaje</th>
            <td>{{ entity.mensaje }}</td>
        </tr>
        <tr>
            <th>Date_create</th>
            <td>{{ entity.datecreate|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admincloud_notificaciones') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admincloud_notificaciones_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admincloud_notificaciones_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
