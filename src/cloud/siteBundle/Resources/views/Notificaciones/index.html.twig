<h1>Notificaciones list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Asunto</th>
            <th>Mensaje</th>
            <th>Date_create</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admincloud_notificaciones_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.asunto }}</td>
            <td>{{ entity.mensaje }}</td>
            <td>{% if entity.datecreate %}{{ entity.datecreate|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admincloud_notificaciones_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admincloud_notificaciones_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admincloud_notificaciones_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
