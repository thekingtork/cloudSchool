<h1>Actividad</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Porcentaje</th>
            <td>{{ entity.Porcentaje }}</td>
        </tr>
        <tr>
            <th>Publicar</th>
            <td>{{ entity.publicar }}</td>
        </tr>
        <tr>
            <th>Categoria</th>
            <td>{{ entity.categoria }}</td>
        </tr>
        <tr>
            <th>Tipo</th>
            <td>{{ entity.tipo }}</td>
        </tr>
        <tr>
            <th>Date_inicio</th>
            <td>{{ entity.dateinicio|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Date_final</th>
            <td>{{ entity.datefinal|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        <tr>
            <th>Logro_competencia</th>
            <td>{{ entity.logrocompetencia }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admincloud_actividad') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admincloud_actividad_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admincloud_actividad_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
