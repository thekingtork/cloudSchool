<h1>Sede list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Capacidad</th>
            <th>Direccion</th>
            <th>Telefono</th>
            <th>Fax</th>
            <th>Email</th>
            <th>Rector</th>
            <th>Secretaria</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admincloud_sede_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.capacidad }}</td>
            <td>{{ entity.direccion }}</td>
            <td>{{ entity.telefono }}</td>
            <td>{{ entity.fax }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.rector }}</td>
            <td>{{ entity.secretaria }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admincloud_sede_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admincloud_sede_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admincloud_sede_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
