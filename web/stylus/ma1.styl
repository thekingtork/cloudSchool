#bloqueCgenerales
    display inline-block
    height 100%
    vertical-align top
    overflow hidden
    width 100%
#contenidoDer
    background url('../../images/backgroudAjax.svg') no-repeat
    background-position 0 50%
    height 86%
    overflow hidden
    width 65%
#contenidoDer,#contenidoIzq
    display inline-block
    vertical-align top
#contenidoIzq
    float left
    overflow hidden
    width 30%
    label
        color rgb(255,255,255)
        display inline
        font-family "Segoe UI light"
        font-size 32px
#ContTitulo
    float left
    margin 68% 10px 0 10px
    p 
        color rgb(255,255,255)
        font-family "Segoe UI light"
        font-size 32px
        line-height 1
        text-align center

#menuAux
    display inline-block
    height 80%
    vertical-align top
    width 100%
    ul
        display inline-block
        left 30%
        list-style none
        margin 0
        position relative
        top 40%
        li
            margin-bottom 20px
            a
                color rgb(255,255,255)
                font-family "Segoe UI light"
                font-size 1.7em
                text-decoration none
