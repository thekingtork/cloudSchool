alinear()
	display inline-block
	vertical-align top
transformar(arg)
	-o-transform rotate(arg)
	-moz-transform rotate(arg)
	-webkit-transform rotate(arg)
	transform rotate(arg)
itemInput(anchoId,anchoP,anchoT)
		width anchoId
		p,input
			alinear()
		p
			width anchoP
		input
			width anchoT
itemSelect(anchoId,anchoP,anchoT)
		width anchoId
		p,select
			alinear()
		p
			width anchoP
		select
			height 27px
			width anchoT