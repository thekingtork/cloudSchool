@import "clasesLima.styl"
#botonesDow
    margin auto
    position relative
    top 0em
    width 60%
    button
        color Lima
    input[type="button"]
        color Lima
.breadcumbr a
    color Lima
.Inicio
    right 37px
    top 10px
    a
        color Lima
.panelInf
    label
        color Lima
        font-family "Segoe UI light"
        line-height 1
    label:first-child
        display inline-block
        font-size 8.5em
    label:nth-child(4n+2)
        display inline-block
        font-size 2.5em
    label:nth-child(4n+3)
        display block
        font-size 1.7em
        margin-left .2em
    label:nth-child(4n+4)
        display block
        font-size 1em
        margin-left 0.385em
.pantalla
    height 231px